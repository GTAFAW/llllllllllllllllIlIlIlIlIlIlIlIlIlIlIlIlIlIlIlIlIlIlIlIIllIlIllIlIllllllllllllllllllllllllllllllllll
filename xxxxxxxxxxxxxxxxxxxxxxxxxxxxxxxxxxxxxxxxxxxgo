		Domain.Home.WallpaperManage.Position = UDim2.new(0.167, 0, 0.548, 0)
		-- Executors
		Domain.Home.Executors.BackgroundTransparency = 1
		Domain.Home.Executors.Title.TextTransparency = 1
		Domain.Home.Executors.Info.TextTransparency = 1
