		tween:Play()
		local transitionInfo = TweenInfo.new(0.5, Enum.EasingStyle.Quint)
		local tween = TweenService:Create(Domain.Home.Discord.Info, transitionInfo, {TextTransparency = 1})
		tween:Play()
		local transitionInfo = TweenInfo.new(0.5, Enum.EasingStyle.Quint)
